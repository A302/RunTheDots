<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.yourdomain.runthedots.Mysterie2intro" >


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView11"
        android:layout_centerVertical="true"
        android:alpha="0.5"
        android:background="@mipmap/background" />


    <ImageButton
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:text="@string/start_game"
        android:id="@+id/button"
        android:layout_marginBottom="60dp"
        android:textSize="50sp"
        android:onClick="Startgame2"
        android:background="@mipmap/start1"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />



    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="225dp"
        android:id="@+id/imageView8"
        android:background="@mipmap/mystery2text"
        android:layout_above="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="52dp" />

    <ImageView
        android:layout_width="205dp"
        android:layout_height="65dp"
        android:id="@+id/Mystery2Title"
        android:background="@mipmap/mysterytext2"
        android:layout_alignTop="@+id/imageView11"
        android:layout_centerHorizontal="true" />
</RelativeLayout>


